[{
  "id": "basic",
  "label": "Basic Workflow",
  "sdesc": "Short description for Basic Workflow",
  "description": "Long description for Basic Workflow. You can include HTML",
  "prj_workspace": {
    "expdir": "exps",
    "namdir": "names",
    "reldir": "rels",
    "rstdir": "reports",
    "logdir": "logs"    
  },
  "replacements": {
    "peptidesQ":          "p2q_outStats",
    "peptides":           "p2a_outStats",
    "proteinsC":          "q2c_outStats",
    "proteins":           "q2a_outStats",
    "categories":         "c2a_outStats",

    "peptidesQ_lnV":          "p2q_lowerNormV",
    "peptides_lnV":           "p2a_lowerNormV",
    "proteinsC_lnV":          "q2c_lowerNormV",
    "proteins_lnV":           "q2a_lowerNormV",
    "categories_lnV":         "c2a_lowerNormV",

    "Vq = 1/(1/V1+1/V2)": "form",
    "Vq = Max(V1,V2)":    "max",
    "Vq = Avg(V1,V2)":    "avg"
  },
  "works": [{
    "id": "main_inputs",
    "label": "Inputs",
    "description": "Short description for the current work (tab)",
    "file": "main_inputs.tsv",
    "cmds": [{
      "id": "CREATE_ID",
      "label": "Select Input Files",
      "visible": true,
      "panel": "panels/main_inputs.html",
      "params": [
        { "type": "required", "name": "infile" },
        { "type": "required", "name": "experiment" }
      ]
    }]
  },{
    "id": "identification",
    "label": "Identification",
    "description": "Short description for the current work (tab)",
    "file": "identification.tsv",
    "cmds": [{
      "id": "FDR",
      "label": "FDR",
      "visible": true,
      "panel": "panels/advance.html",
      "params": [
        { "type": "required", "name": "experiment" },
        { "type": "required", "name": "threshold" },
        { "type": "required", "name": "jump_areas" },
        { "type": "required", "name": "lab_decoy" },
        { "type": "required", "name": "fdr" },
        { "type": "required", "name": "type_xcorr" },
        { "type": "optional", "name": "more_params" }
      ]
    },{
      "id": "MASTERQ",
      "label": "Rels. p2q",
      "visible": true,
      "panel": "panels/advance.html",
      "params": [
        { "type": "required", "name": "experiment", "readOnly": true },
        { "type": "required", "name": "pretxt" },
        { "type": "required", "name": "lab_decoy" },
        { "type": "optional", "name": "more_params" }
      ]
    },{
      "id": "RELS_TABLE_SPEC",
      "label": "Rels. s2p",
      "visible": true,
      "panel": "panels/advance.html",
      "params": [
        { "type": "required", "name": "input",  "readOnly": true },
        { "type": "required", "name": "output", "readOnly": true },
        { "type": "required", "name": "inf_header" },
        { "type": "required", "name": "sup_header" },
        { "type": "required", "name": "3rd_headers" },
        { "type": "optional", "name": "more_params" }
      ]
    }]
  },{
    "id": "stats_quantification",
    "label": "Stats Quantification",
    "description": "Short description for the current work (tab)",
    "file": "stats_quantification.tsv",
    "cmds": [{
      "id": "RATIOS_WSPP-WSPP_SBT",
      "label": "WSPP - SBT",
      "visible": true,
      "panel": "panels/advance.html",
      "params": [
        { "type": "required", "name": "experiment" },
        { "type": "required", "name": "name" },
        { "type": "required", "name": "ratio_numerator" },
        { "type": "required", "name": "ratio_denominator" },
        { "type": "optional", "name": "p>q_Tag" },
        { "type": "optional", "name": "p>a_Tag" },
        { "type": "optional", "name": "c>a_Tag" },
        { "type": "optional", "name": "s>p_FDR" },
        { "type": "optional", "name": "p>q_FDR" },
        { "type": "optional", "name": "q>c_FDR" },
        { "type": "optional", "name": "s>p_Var(x)" },
        { "type": "optional", "name": "p>q_Var(x)" },
        { "type": "optional", "name": "q>c_Var(x)" },
        { "type": "optional", "name": "more_params" }
      ]
    }]
  },{
    "id": "integration",
    "label": "Integration",
    "description": "Short description for the current work (tab)",
    "file": "integration.tsv",
    "cmds": [{
      "id": "COMBINE",
      "label": "Combine",
      "visible": true,
      "panel": "panels/advance.html",
      "params": [
        { "type": "required", "name": "input" },
        { "type": "required", "name": "output" },
        { "type": "required", "name": "level", "select": ["p2q_lowerNormV", "p2a_lowerNormV", "q2c_lowerNormV", "q2a_lowerNormV", "c2a_lowerNormV"] },
        { "type": "optional", "name": "more_params" }
      ]
    },{
      "id": "RELS_TABLE",
      "label": "Create Rels.",
      "visible": true,
      "panel": "panels/advance.html",
      "params": [
        { "type": "required", "name": "input" },
        { "type": "required", "name": "inf_header" },
        { "type": "required", "name": "sup_header" },
        { "type": "required", "name": "3rd_headers" },
        { "type": "required", "name": "output" },
        { "type": "optional", "name": "more_params" }
      ]
    },{
      "id": "RATIOS_INT",
      "label": "Calculate ratios and integrate",
      "visible": true,
      "panel": "panels/advance.html",
      "params": [
        { "type": "required", "name": "name" },
        { "type": "required", "name": "ratio_numerator" },
        { "type": "required", "name": "ratio_denominator" },
        { "type": "required", "name": "level", "select": ["p2q_lowerNormV", "p2a_lowerNormV", "q2c_lowerNormV", "q2a_lowerNormV", "c2a_lowerNormV"] },
        { "type": "required", "name": "type", "select": ["form", "max", "avg"] },
        { "type": "required", "name": "rel_table" },
        { "type": "optional", "name": "Tag" },
        { "type": "optional", "name": "FDR" },
        { "type": "optional", "name": "Var(x)" },
        { "type": "optional", "name": "more_params" }
      ]
    },{
      "id": "INTEGRATE",
      "label": "Integrate",
      "visible": true,
      "panel": "panels/advance.html",
      "params": [
        { "type": "required", "name": "input" },
        { "type": "required", "name": "output" },
        { "type": "required", "name": "rel_table" },
        { "type": "optional", "name": "Tag" },
        { "type": "optional", "name": "FDR" },
        { "type": "optional", "name": "Var(x)" },
        { "type": "optional", "name": "more_params" }
      ]
    }]
  },{
    "id": "reports",
    "label": "Reports",
    "description": "Short description for the current work (tab)",
    "file": "reports.tsv",
    "cmds": [{
      "id": "REPORT",
      "label": "Report",
      "visible": true,
      "panel": "panels/advance.html",
      "params": [
        { "type": "required", "name": "input" },
        { "type": "required", "name": "output" },
        { "type": "required", "name": "inf_level", "select": ["p2q_outStats", "p2a_outStats", "q2c_outStats", "q2a_outStats", "c2a_outStats"] },
        { "type": "required", "name": "sup_level", "select": ["p2q_outStats", "p2a_outStats", "q2c_outStats", "q2a_outStats", "c2a_outStats"] },
        { "type": "optional", "name": "more_params" }
      ]
    }]
  }]
},{
